<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.xinghua1.xinghua1__fueltrack">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddNewEntryActivity"
            android:label="@string/title_activity_add_new_entry" />
        <activity
            android:name=".ViewListActivity"
            android:label="@string/title_activity_view_list" />
        <activity
            android:name=".EditActivity"
            android:label="@string/title_activity_edit"></activity>
    </application>

</manifest>
